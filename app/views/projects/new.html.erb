<h1>New Project</h1>

<%= form_with(model: @project, url: projects_path, local: true) do |form| %>
  <div class="field">
    <%= form.label :title %>
    <%= form.text_field :title, required: true %>
  </div>

  <div class="field">
    <%= form.label :description %>
    <%= form.text_area :description, required: true %>
  </div>

  <div class="field">
    <%= form.label :technologies_used %>
    <%= form.text_field :technologies_used %>
  </div>

  <!-- Add more fields as needed -->

  <div class="actions">
    <%= form.submit 'Create Project' %>
  </div>
<% end %>

<%= link_to 'Back to Projects', projects_path %>
